<h1><%= pluralize(@users.size,"User") %></h1>
<ul class="users">
    <% @users.each do |user|%>
        <li>
            <%= link_to user.name.capitalize, user %>
            created
            <%= time_ago_in_words(user.created_at)%>
        </li>
    <% end %>
</ul>

<div>
    <%= link_to "Create User", signup_path, class:"button"%>
</div>
